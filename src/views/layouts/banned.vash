<!DOCTYPE html>
    <html>
    <head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@model.title - Hindi Gamer Club</title>
    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,600,700,900" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@@4.3.1/dist/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <!-- Local CSS -->
    <link href="/css/custom.css?v=@model.v" rel="stylesheet">
    <link href="/css/animate.css?v=@model.v" rel="stylesheet">

        <!-- Metrics -->
        <script nonce="@model.nonce" src="/js/metrics.js?v=@model.v"></script>
        <!-- Font Awesome -->
        <script nonce="@model.nonce" src="https://kit.fontawesome.com/983cb40861.js"></script>
        <!-- Recaptcha -->
        <script nonce="@model.nonce" src="https://www.google.com/recaptcha/api.js" async defer></script>
    <style>
        body,html {
            margin: 0;
            padding: 0;
            background: #1d1d23;
            font-family: 'Montserrat', sans-serif;
        }
        div.container {
            margin-top:1rem;
        }
        h5 {
            font-weight: 700;
        }
        p {
            margin-bottom: 0;
        }
    </style>
    @html.block('header')
    </head>
    <body>
        @if (model.userInfo) {
            <div 
                    id="userdata" 
                    data-authenticated="true" 
                    data-csrf="@model.csrf" 
                    data-userid="@model.userInfo.userId" 
                    data-username="@model.userInfo.username" 
            ></div>
        }else{
            <div data-authenticated="false" data-csrf="@model.csrf"  id="userdata"> </div>
        }
        
        <div class="container">
            @html.block('content')
        </div>

        <script nonce="@model.nonce" src="https://browser.sentry-cdn.com/5.12.1/bundle.min.js" integrity="sha384-y+an4eARFKvjzOivf/Z7JtMJhaN6b+lLQ5oFbBbUwZNNVir39cYtkjW1r6Xjbxg3" crossorigin="anonymous"></script>
        <script nonce="@model.nonce">
            Sentry.init({ dsn: 'https://a5c3a9adef4a4e149a1e2d1651b9da4d@sentry.io/2505702' });
        </script>
        <!-- Optional JavaScript -->
        <!-- jQuery first, then Popper.js, then Bootstrap JS -->
        <script nonce="@model.nonce" src="https://cdn.jsdelivr.net/npm/jquery@@3.4.1/dist/jquery.min.js" integrity="sha384-vk5WoKIaW/vJyUAd9n/wmopsmNhiy+L2Z+SBxGYnUkunIxVxAv/UtMOhba/xskxh" crossorigin="anonymous"></script>
        <!-- swal2 -->
        <script nonce="@model.nonce" src="https://cdn.jsdelivr.net/npm/sweetalert2@@8.17.1/dist/sweetalert2.all.min.js" integrity="sha256-VxbmCDobjdVsaHYJOlEcQcc/p89LW76+b4x3r7l5Ikg=" crossorigin="anonymous"></script>
        <!-- popper js -->
        <script nonce="@model.nonce" src="https://cdn.jsdelivr.net/npm/popper.js@@1.14.4/dist/umd/popper.min.js" integrity="sha256-EGs9T1xMHdvM1geM8jPpoo8EZ1V1VRsmcJz8OByENLA=" crossorigin="anonymous"></script>
        <!-- Bootstrap -->
        <script nonce="@model.nonce" src="https://cdn.jsdelivr.net/npm/bootstrap@@4.3.1/dist/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
        <!-- Moment -->
        <script nonce="@model.nonce" src="https://cdn.jsdelivr.net/npm/moment@@2.24.0/moment.min.js" crossorigin="anonymous"></script>
        <!-- XSS -->
        <script nonce="@model.nonce" src="/js/xss.js?v=@model.v"></script>
        <!-- Locale -->
        <script nonce="@model.nonce" src="/js/locale/en.js?v=@model.v"></script>
        <!-- Local Stuff -->
        <script nonce="@model.nonce" src="/js/app.js?v=@model.v"></script>
        @if (model.userInfo && typeof model.userInfo.userId !== "undefined" && model.userInfo.userId !== null) {
        <!-- Chat Stuff -->
            <script nonce="@model.nonce" src="/js/chat.js?v=@model.v"></script>
        }

        @html.block('js')
    </body>
</html>