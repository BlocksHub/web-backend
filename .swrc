{
  "test": ".*.ts$",
  "jsc": {
    target: 'es2019',
    "parser": {
        "syntax": "typescript",
        "tsx": false,
        "decorators": true,
        "dynamicImport": true
    },
    "module": {
        "type": "commonjs",

        // These are defaults.
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    }
}